{
  "name": "movement-pass-public-api",
  "version": "0.1.0",
  "private": true,
  "main": "build/src/lambda.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/local.ts",
    "format": "prettier --write **/*.ts",
    "lint": "eslint ./ --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "release": "npm run format && npm run lint && npm run test && npm run build && npm run pack",
    "deploy": "npm run release && cdk deploy --require-approval never --profile movement-pass",
    "destroy": "cdk destroy --profile movement-pass"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.100.0",
    "@aws-sdk/client-s3": "^3.100.0",
    "@aws-sdk/client-ssm": "^3.100.0",
    "@aws-sdk/lib-dynamodb": "^3.100.0",
    "@aws-sdk/s3-request-presigner": "^3.100.0",
    "@vendia/serverless-express": "^4.8.0",
    "aws-lambda": "^1.0.7",
    "aws-xray-sdk": "^3.3.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.159.0",
    "@aws-cdk/aws-certificatemanager": "^1.159.0",
    "@aws-cdk/aws-dynamodb": "^1.159.0",
    "@aws-cdk/aws-iam": "^1.159.0",
    "@aws-cdk/aws-lambda": "^1.159.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.159.0",
    "@aws-cdk/aws-route53": "^1.159.0",
    "@aws-cdk/aws-route53-targets": "^1.159.0",
    "@aws-cdk/aws-s3": "^1.159.0",
    "@aws-cdk/aws-sqs": "^1.159.0",
    "@aws-cdk/aws-ssm": "^1.159.0",
    "@aws-cdk/core": "^1.159.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
